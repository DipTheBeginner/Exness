{
  "name": "exness",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.3.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "bun --cwd packages/database prisma studio"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prisma": "5.22.0",
    "turbo": "^2.3.3"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.2.7"
  }
}